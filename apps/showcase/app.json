{
  "name": "Fox UI",
  "description": "A Demo React Native fox UI based application",
  "slug": "fox-ui",
  "version": "1.0.0",
  "scheme": "fox-ui",
  "orientation": "portrait",
  "icon": "./src/assets/images/icon.png",
  "userInterfaceStyle": "light",
  "newArchEnabled": true,
  "assetBundlePatterns": [
    "**/*"
  ],
  "platforms": [
    "web"
  ],
  "web": {
    "favicon": "./src/assets/images/favicon.png",
    "bundler": "metro"
  },
  "extra": {},
  "experiments": {
    "tsconfigPaths": true,
    "typedRoutes": true
  },
  "owner": "vedla",
  "runtimeVersion": {
    "policy": "appVersion"
  },
  "plugins": [
    "expo-router",
    "expo-web-browser"
  ]
}