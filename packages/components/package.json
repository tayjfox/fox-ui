{
  "name": "@fox-ui/components",
  "version": "1.0.0",
  "description": "React Native components based on Eva Design System",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./Button": {
      "import": "./src/Button/index.ts",
      "types": "./dist/Button/index.d.ts",
      "require": "./src/Button/index.cjs",
      "default": "./src/Button/index.ts"
    },
    "./Text": {
      "import": "./src/Text/index.ts",
      "types": "./dist/Text/index.d.ts",
      "require": "./src/Text/index.cjs",
      "default": "./src/Text/index.ts"
    },
    "./Renderable": {
      "import": "./src/Renderable/index.ts",
      "types": "./dist/Renderable/index.d.ts",
      "require": "./src/Renderable/index.cjs",
      "default": "./src/Renderable/index.ts"
    },
    "./Touchable": {
      "import": "./src/Touchable/index.ts",
      "types": "./dist/Touchable/index.d.ts",
      "require": "./src/Touchable/index.cjs",
      "default": "./src/Touchable/index.ts"
    },
    "./Icon": {
      "import": "./src/Icon/index.ts",
      "types": "./dist/Icon/index.d.ts",
      "require": "./src/Icon/index.cjs",
      "default": "./src/Icon/index.ts"
    },
    "./Layout": {
      "import": "./src/Layout/index.ts",
      "types": "./dist/Layout/index.d.ts",
      "require": "./src/Layout/index.cjs",
      "default": "./src/Layout/index.ts"
    }
  },
  "main": "./src/index.tsx",
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "dev": "tsup --watch"
  },
  "dependencies": {
    "fecha": "4.2.3",
    "hoist-non-react-statics": "^3.3.2"
  },
  "devDependencies": {
    "tsup": "8.5.0"
  },
  "peerDependencies": {
    "@fox-ui/utils": "*",
    "@shopify/flash-list": ">2.0.0",
    "@types/hoist-non-react-statics": "^3.3.7",
    "react": ">=19.0.0",
    "react-dom": ">=19.1.0",
    "react-native": ">=0.70.0",
    "react-native-reanimated": ">=4.1.0",
    "react-native-svg": ">=15.12.1",
    "react-native-web": ">=0.21.1",
    "tailwindcss": "^4.1.14"
  }
}
